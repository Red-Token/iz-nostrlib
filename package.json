    "ts-node": "^10.9.2",
    "tsc-multi": "^1.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.25.0"
  },
  "mocha": {
    "node-option": [
      "import=tsx"
    ]
  },
  "dependencies": {
    "@noble/hashes": "^1.5.0",
    "@red-token/mitt": "^3.0.3",
    "@red-token/welshman": "^0.2.8",
    "deepmerge": "^4.3.1",
    "node-ssh": "^13.2.0",
    "nostr-tools": "^2.10.3"
  }
